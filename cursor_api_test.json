{
  "version": "1.0",
  "name": "Cursor API Collection",
  "description": "Cursor API 接口测试集合",
  "variables": [
    {
      "name": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "name": "authToken",
      "value": "e4de3277818ec8f0e034e2abc1945f09531576851ae0aec789cd512cb49d9eb0"
    }
  ],
  "folders": [
    {
      "name": "基础接口",
      "requests": [
        {
          "name": "健康检查",
          "method": "GET",
          "url": "{{baseUrl}}/health",
          "headers": [],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取版本信息",
          "method": "GET",
          "url": "{{baseUrl}}/",
          "headers": [],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        }
      ]
    },
    {
      "name": "静态资源",
      "requests": [
        {
          "name": "获取共享样式",
          "method": "GET",
          "url": "{{baseUrl}}/static/shared-styles.css",
          "headers": [],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取共享脚本",
          "method": "GET",
          "url": "{{baseUrl}}/static/shared.js",
          "headers": [],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取环境变量示例",
          "method": "GET",
          "url": "{{baseUrl}}/env-example",
          "headers": [],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        }
      ]
    },
    {
      "name": "模型与配置",
      "requests": [
        {
          "name": "获取模型列表",
          "method": "GET",
          "url": "{{baseUrl}}/v1/models",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            }
          ],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取随机hash",
          "method": "GET",
          "url": "{{baseUrl}}/get-hash",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            }
          ],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取checksum",
          "method": "GET",
          "url": "{{baseUrl}}/get-checksum",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            }
          ],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取tsheader",
          "method": "GET",
          "url": "{{baseUrl}}/get-tsheader",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            }
          ],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        }
      ]
    },
    {
      "name": "日志与用户信息",
      "requests": [
        {
          "name": "获取日志",
          "method": "GET",
          "url": "{{baseUrl}}/logs",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            }
          ],
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "提交日志数据",
          "method": "POST",
          "url": "{{baseUrl}}/logs",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{authToken}}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{}"
          },
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        },
        {
          "name": "获取用户信息",
          "method": "POST",
          "url": "{{baseUrl}}/userinfo",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\"token\": \"{{authToken}}\"}"
          },
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        }
      ]
    },
    {
      "name": "校准与配置",
      "requests": [
        {
          "name": "基础校准",
          "method": "POST",
          "url": "{{baseUrl}}/basic-calibration",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\"token\": \"{{authToken}}\"}"
          },
          "assertions": [
            {
              "assertion": "response.status === 200"
            }
          ]
        }
      ]
    }
  ]
}